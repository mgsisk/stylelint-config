{
  "name": "@mgsisk/stylelint-config",
  "version": "0.1.3",
  "description": "Opinionated stylelint configurations for CSS development.",
  "main": "index.js",
  "scripts": {
    "clean": "find . -name '*.DS_Store' -type f -delete",
    "preversion": "npm test",
    "postversion": "git push --follow-tags && npm publish",
    "fix": "npm run fix.js",
    "fix.js": "eslint --fix '*.js' '.*.js'",
    "test": "npm run test.js",
    "test.js": "eslint '*.js' '.*.js'",
    "test.json": "find . -name '*.json' ! -path '*/node_modules/*' | xargs -L1 jsonlint -q",
    "test.md": "markdownlint ./**/*.md -i node_modules/ code_of_conduct.md license.md",
    "test.write": "find . -name '*.md' ! -path '*/node_modules/*' ! -name 'license.md' ! -name 'code_of_conduct.md' | xargs -L1 write-good",
    "test.yml": "find . -name '*.yml' ! -path '*/node_modules/*' | xargs -L1 js-yaml"
  },
  "repository": "github:mgsisk/stylelint-config",
  "keywords": [
    "css",
    "lint",
    "linter",
    "linting",
    "stylelint",
    "stylelintconfig",
    "stylelint-config",
    "stylelint-config-wordpress",
    "wordpress"
  ],
  "author": "mgsisk <me@mgsisk.com> (https://mgsisk.com)",
  "license": "ISC",
  "bugs": {
    "email": "help@mgsisk.com",
    "url": "https://github.com/mgsisk/stylelint-config/issues"
  },
  "homepage": "https://github.com/mgsisk/stylelint-config#readme",
  "dependencies": {
    "stylelint-a11y": "^1.1.9",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-no-indistinguishable-colors": "^1.2.1",
    "stylelint-order": "^2.0.0"
  },
  "devDependencies": {
    "@mgsisk/eslint-config": "^0.3.1",
    "eslint": "^5.9.0",
    "js-yaml": "^3.12.0",
    "jsonlint": "^1.6.3",
    "markdownlint-cli": "^0.13.0",
    "write-good": "^0.13.1"
  },
  "peerDependencies": {
    "stylelint": ">= 9.9.0"
  }
}
